#constant stroke width
#constand transparency
#but random stroke and fill colors

dia = 100
for yFactor in range(10):
    posY = yFactor * dia
    for xFactor in range(10):
        posX = xFactor * dia
#invulling van het raster
        d = random() * dia
        x = posX + (dia - d) / 2
        y = posY + (dia - d) / 2
        fill(random(),random(),random())
        strokeWidth(8)
        stroke(random(), random(), random())
        oval(x,y,d,d)

